{
  "schemaVersion": 1,
  "id": "remasteredcore",
  "version": "${version}",

  "name": "RemasteredCore",
  "description": "This is a core mod for the modpack Survival Remastered. Not recommended to use in any other modpack unless you know what you are doing.",
  "authors": [
    "Treetrain1"
  ],
  "contact": {
    "homepage": "https://github.com/Treetrain1/RemasteredCore",
    "sources": "https://github.com/Treetrain1/RemasteredCore",
    "issues": "https://github.com/Treetrain1/RemasteredCore/issues"
  },

  "license": "LGPL-3.0",
  "icon": "assets/remasteredcore/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.frozenblock.remasteredcore.RemasteredCore"
    ],
    "client": [
      "net.frozenblock.remasteredcore.RemasteredCoreClient"
    ]
  },
  "mixins": [
	"remasteredcore.mixins.json"
  ],

  "accessWidener": "remasteredcore.accesswidener",

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": ">=0.58.0",
    "minecraft": "~1.19",
    "java": ">=17"
  },

  "breaks": {
    "quilted_fabric_api": "*"
  }
}
